

@model IEnumerable<LeagueStore.Models.Employee>
<ul>
    @foreach (var employee in Model)
    {
        <li>@employee.Name</li>
    }
</ul>







        
            <div class="col-md-6">
                <div class="box employee">

                    <img  src="../@employee.ImageUrl">
                    <h2>@employee.Name</h2>
                    <h4>@employee.Role</h4>
            </div>
        </div>
        }
